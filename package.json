{
  "name": "promptly-api",
  "version": "1.0.0",
  "description": "Backend API for Promptly - AI-powered Hinge prompt generator",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production ts-node src/server.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "vercel-build": "npm run build"
  },
  "keywords": [
    "api",
    "nodejs",
    "express",
    "typescript",
    "ai",
    "dating"
  ],
  "author": "Promptly Team",
  "license": "MIT",
  "dependencies": {
    "@clerk/express": "^1.6.0",
    "@prisma/client": "^5.6.0",
    "@supabase/supabase-js": "^2.50.0",
    "@types/uuid": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "helmet": "^7.2.0",
    "openai": "^4.20.1",
    "prisma": "^5.6.0",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.22",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.19.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
